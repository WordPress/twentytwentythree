{
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "page-large-header",
			"postTypes": [
				"page"
			],
			"title": "Page (Large Header)"
		},
		{
			"name": "single-no-separators",
			"postTypes": [
				"post"
			],
			"title": "Single Post (No Separators)"
		},
		{
			"name": "page-no-separators",
			"postTypes": [
				"page"
			],
			"title": "Page (No Separators)"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#cccccc",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#333333",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffffff40",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"layout": {
			"contentSize": "80vw",
			"wideSize": "90vw"
		},
		"spacing": {
			"spacingScale": {
				"operator": "+",
				"increment": 0.5,
				"steps": 9,
				"mediumStep": 3,
				"unit": "vw"
			},
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"size": "2.5rem",
					"slug": "large"
				},
				{
					"size": "4rem",
					"slug": "x-large"
				},
				{
					"size": "5.5rem",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/calendar": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "overline 1px"
						}
					}
				}
			},
			"core/code": {
				"border": {
					"radius": "var(--wp--preset--spacing--30)"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary)"
				},
				"spacing": {
					"padding": "var(--wp--style--block-gap)"
				}
			},
			"core/image": {
				"border": {
					"color": "var(--wp--preset--color--tertiary)",
					"radius": "var(--wp--preset--spacing--10)",
					"style": "solid",
					"width": "var(--wp--preset--spacing--10)"
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--50)"
				},
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--base)"
							},
							"typography": {
								"textDecoration": "overline 1px"
							}
						}
					}
				}
			},
			"core/post-author": {
				"border": {
					"left": {
						"color": "currentColor",
						"style": "solid",
						"width": "1px"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--style--block-gap)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "200"
				}
			},
			"core/post-comments": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "overline dashed 1px"
							}
						}
					}
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "overline dashed 1px"
							}
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"color": "var(--wp--preset--color--tertiary)",
					"radius": "var(--wp--preset--spacing--10)",
					"style": "solid",
					"width": "var(--wp--preset--spacing--10)"
				}
			},
			"core/post-terms": {
				"border": {
					"left": {
						"color": "currentColor",
						"style": "solid",
						"width": "1px"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--style--block-gap)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": "0px"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.2"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"top": {
						"color": "transparent",
						"width": "8vw"
					},
					"bottom": {
						"color": "transparent",
						"width": "8vw"
					},
					"color": "var(--wp--preset--color--tertiary)",
					"style": "double",
					"width": "8vw"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/quote": {
				"color": {
					"background": "var(--wp--preset--color--tertiary)"
				},
				"border": {
					"radius": "var(--wp--preset--spacing--10)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--70)",
						"right": "var(--wp--preset--spacing--70)",
						"top": "var(--wp--preset--spacing--10)"
					}
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"color": "transparent",
						"width": "10vw"
					},
					"top": {
						"color": "transparent",
						"width": "10vw"
					},
					"style": "double",
					"width": "40vw"
				},
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "200"
				}
			},
			"core/template-part": {
				"color": {
					"gradient": "linear-gradient(180deg, rgba(0,204,204,0.5) 0%, transparent 100%)"
				}
			},
			"core/verse": {
				"border": {
					"radius": "var(--wp--preset--spacing--30)"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary)"
				},
				"spacing": {
					"padding": "var(--wp--style--block-gap)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"gradient": "radial-gradient(ellipse at top left, rgba(236, 249, 87, 0.5) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom left, rgba(247, 69, 204, 0.5) 15% , transparent 100%) fixed, radial-gradient(ellipse at top right, rgba(121, 238, 196, 0.5) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom right, rgba(81, 82, 213, 0.5) 15% , transparent 100%) fixed",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "99999px"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary)",
					"gradient": "radial-gradient(ellipse at bottom right, rgba(236, 249, 87, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at top right, rgba(247, 69, 204, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom left, rgba(121, 238, 196, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom left, rgba(81, 82, 213, 1) 15% , transparent 100%) fixed",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontWeight": "200",
					"letterSpacing": ".2em",
					"textTransform": "uppercase"
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)",
						"gradient": "radial-gradient(ellipse at top left, rgba(236, 249, 87, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom left, rgba(247, 69, 204, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at top right, rgba(121, 238, 196, 1) 15% , transparent 100%) fixed, radial-gradient(ellipse at bottom right, rgba(81, 82, 213, 1) 15% , transparent 100%) fixed"
					}
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "100",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "100",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--style--block-gap)"
					}
				},
				"typography": {
					"fontWeight": "200"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"textDecoration": "overline 1px"
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "overline 1px"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "4vw"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "200",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
