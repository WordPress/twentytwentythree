{
	"title": "Aubergine",
	"settings": {
		"color": {
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--base) 100%)",
					"name": "Secondary to Base",
					"slug": "secondary-base"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--tertiary) 5.74%, var(--wp--preset--color--primary) 100%);",
					"name": "Tertiary to Primary",
					"slug": "tertiary-primary"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--primary) 5.74%, var(--wp--preset--color--tertiary) 100%);",
					"name": "Primary to Tertiary",
					"slug": "primary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#1B1031",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#FFFFFF",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#FF746D",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#551C5E",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FB326B",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/comment-reply-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"fontStyle": "italic"
						}
					}
				}
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"fontStyle": "italic"
						}
					}
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"textTransform": "uppercase"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"fontStyle": "italic"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"core/separator": {
				"border": {
					"width": "2px"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"color": {
			"gradient": "var(--wp--preset--gradient--secondary-base)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "99999px"
				},
				"color": {
					"gradient": "var(--wp--preset--gradient--tertiary-primary)",
					"text": "var(--wp--preset--color--base)"
				}
			},
			"link": {
				":active": {
					"color": {
						"text": "var(--wp--preset--color--tertiary)"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-sans)"
		}
	},
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
